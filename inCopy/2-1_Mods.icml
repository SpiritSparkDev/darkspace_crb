		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Eigenschaften%3aEigenschaftText">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Durch zwei Aktivierungen wird das Nebenergebnis der Probe auf das Hauptergebnis gesetzt.</Content>
				<Br />
			</CharacterStyleRange>
		</ParagraphStyleRange>
		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Überschriften%3aHeading 3">
