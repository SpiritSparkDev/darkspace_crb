			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Gegenstände und Fertigkeiten</Content>
				<Br />
			</CharacterStyleRange>
		</ParagraphStyleRange>
