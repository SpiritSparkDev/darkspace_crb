		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/$ID/NormalParagraphStyle">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Fernkampfwaffen und Terminals haben eine Reichweite. </Content>
				<Br />
			</CharacterStyleRange>
		</ParagraphStyleRange>
		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Regeltexte%3aRegelfeld">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Ideale Reichweite: Angriff auf genau diese Reichweite werden nach den normalen Regeln ausgeführt.</Content>
				<Br />
				<Content>Anlegen: Angriffe auf eine Reichweite, die um eine Kategorie abweicht, kann als erschwerte Probe durchgeführt werden. </Content>
				<Br />
			</CharacterStyleRange>
		</ParagraphStyleRange>
		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Regeltexte%3aRegelfeldAufzählung">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Nimmt der Charakter 1 Punkt Stress auf sich, wird die Probe normal durchgeführt.</Content>
				<Br />
			</CharacterStyleRange>
		</ParagraphStyleRange>
		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Regeltexte%3aRegelfeld">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Meisterschuss: Angriffe auf eine Reichweite, die um zwei Kategorien abweicht, kann als verzweifelte Probe durchgeführt werden. </Content>
				<Br />
			</CharacterStyleRange>
		</ParagraphStyleRange>
		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Regeltexte%3aRegelfeldAufzählung">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
