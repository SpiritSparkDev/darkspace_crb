					<Cell Self="u9a217ibd920i5" Name="1:3" RowSpan="1" ColumnSpan="1" CellType="TextTypeCell" TextTopInset="4" TextLeftInset="4" TextBottomInset="4" TextRightInset="4" ClipContentToTextCell="false" AppliedCellStyle="CellStyle/$ID/[None]" AppliedCellStylePriority="2" LeftInset="4" TopInset="4" RightInset="4" BottomInset="4" ClipContentToCell="false">
						<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/$ID/NormalParagraphStyle">
							<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
								<Content>Schwerer Mech: 10 Meter große Riesen. </Content>
