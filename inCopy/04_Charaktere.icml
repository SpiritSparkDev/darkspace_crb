		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Standardformate%3aNormal">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Diese Liste an Basis-Mods ist ein Überblick über Mods, die der Charakter während der Erstellung von seinen Start-Erfahrungspunkten kaufen kann.</Content>
				<Br />
				<Content>Eine vollständige Liste aller Mods mit detailierten Regeln findet sich ab </Content>
			</CharacterStyleRange>
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Standardformate%3aItalic" />
			<CrossReferenceSource Self="u5ccd1" AppliedFormat="u9e" Name="Hyperlink 306" Hidden="false" AppliedCharacterStyle="n">
				<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Standardformate%3aItalic">
					<Content>Basis-Mods (S. </Content>
				</CharacterStyleRange>
				<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Standardformate%3aItalic" PageNumberType="TextVariable">
					<TextVariableInstance Self="u615e9" Name="&lt;?AID 001b?&gt;TV XRefPageNumber" ResultText="73" AssociatedTextVariable="dTextVariablen&lt;?AID 001b?&gt;TV XRefPageNumber" />
				</CharacterStyleRange>
				<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Standardformate%3aItalic">
					<Content>)</Content>
				</CharacterStyleRange>
			</CrossReferenceSource>
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Standardformate%3aItalic">
				<Content>.</Content>
