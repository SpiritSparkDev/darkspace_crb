		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Überschriften%3aHeading 4">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Technik</Content>
				<Br />
			</CharacterStyleRange>
		</ParagraphStyleRange>
		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Standardformate%3aNormal">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Technisches Verständnis, handwerkliches Geschick und der Umgang mit Maschinen und Elektrizität wird durch die Technik-Fertigkeit dargestellt. Ein Charakter mit dieser Fertigkeit kann Maschinen, Fahrzeuge und Drohnen bauen und reparieren.</Content>
				<Br />
			</CharacterStyleRange>
		</ParagraphStyleRange>
		<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Regeltexte%3aRegelfeld">
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
				<Content>Technik-Proben mit praktischer Anwendung können nicht ohne </Content>
			</CharacterStyleRange>
			<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Standardformate%3aItalic" />
			<CrossReferenceSource Self="u60042" AppliedFormat="u9e" Name="Hyperlink 296" Hidden="false" AppliedCharacterStyle="n">
				<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Standardformate%3aItalic">
					<Content>Werkzeug (S. </Content>
				</CharacterStyleRange>
				<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Standardformate%3aItalic" PageNumberType="TextVariable">
